{
  "cli": {
    "fmtVersion": 3
  },
  "name": "YFS",
  "description": "Flight Core",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [
    {
      "id": "libs",
      "type": "project",
      "path": "e/lib"
    }
  ],
  "builds": {
    "variants/Unlimited": {
      "name": "variants/Unlimited",
      "type": "control",
      "slots": {
        "receiver": {
          "name": "receiver",
          "type": "receiver"
        },
        "spaceRadar": {
          "name": "spaceRadar",
          "type": "radar"
        },
        "atmoRadar": {
          "name": "atmoRadar",
          "type": "radar"
        }
      }
    },
    "screen/screen": {
      "name": "screen/screen",
      "type": "screen",
      "slots": {}
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "compress": false
    },
    "release": {
      "name": "release",
      "minify": true,
      "compress": true
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}